{
    "name": "decodelabs/dovetail",
    "description": "Comprehensive config solution",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/atlas": "^0.13.1",
        "decodelabs/collections": "^0.11.3",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/fluidity": "^0.3.6",
        "decodelabs/lucid": "^0.5.2",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/veneer": "^0.12.8",

        "vlucas/phpdotenv": "^5.6.2"
    },
    "require-dev": {
        "decodelabs/genesis": "^0.12.4",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/genesis": "<0.12"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Dovetail\\": "src/Dovetail",
            "DecodeLabs\\Genesis\\": "src/Genesis"
        },
        "files": [
            "src/Dovetail/Context.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Dovetail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
