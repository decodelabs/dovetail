{
    "name": "decodelabs/dovetail",
    "description": "Comprehensive config solution",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.4",
        "decodelabs/atlas": "^0.14",
        "decodelabs/collections": "^0.12",
        "decodelabs/coercion": "^0.3.5",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/fluidity": "^0.3.7",
        "decodelabs/iota": "^0.3",
        "decodelabs/kingdom": "^0.1",
        "decodelabs/lucid": "^0.6",
        "decodelabs/monarch": "^0.2",

        "vlucas/phpdotenv": "^5.6.2"
    },
    "require-dev": {
        "decodelabs/genesis": "^0.13",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/genesis": "<0.13"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Dovetail\\": "src/Dovetail",
            "DecodeLabs\\Genesis\\": "src/Genesis"
        },
        "classmap": [
            "src/Dovetail.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Dovetail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
